{
  "days_before_today": 5,

  "properties.scheme": {
    "Nom": {
      "col_type": "title",
      "print_to": "summary",
      "order": 3,
      "title": "Tâche",
      "title_suffix": " : ",
      "title_prefix": "",
      "show_title": false
    },

    "Projet": {
      "col_type": "multi_select",
      "print_to": "summary",
      "prefix": "[",
      "suffix": "]",
      "character_limit": 1,
      "order": 1
    },

    "Importance": {
      "col_type": "select",
      "print_to": "description",
      "order": 2,
      "title": "Importance : ",
      "show_title": true
    },
    "État": {
      "col_type": "status",
      "print_to": "description",
      "order": 1,
      "title": "État : ",
      "show_title": true
    },
    "Date": {
      "col_type": "date"
    },

    "Notes": {
      "col_type": "rich_text",
      "print_to": "description",
      "order": 3,
      "title": "Notes : ",
      "show_title": true
    },
    "Sous-élément": {
      "col_type": "relation",
      "print_to": "summary",
      "order": 2,
      "suffix": " - "
    },
    "élément parent": {
      "col_type": "relation",
      "print_to": "description",
      "order": 5,
      "show_title": true,
      "title": "Sous-tâches : "
    }
  },

  "fields.format": {
    "summary": {
      "prefix": "",
      "suffix": "",
      "inner_separator": " "
    },
    "description": {
      "prefix": "",
      "suffix": "",
      "inner_separator": "\n"
    }
  }
}
